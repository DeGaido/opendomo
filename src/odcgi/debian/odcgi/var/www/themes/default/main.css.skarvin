button, .buttonlnk {
	background:#25A6E1;
	background:-moz-linear-gradient(top,#2A568E 0%,#053F95 100%);
	background:-webkit-gradient(linear,left top,left bottom,color-stop(0%,#2A568E),color-stop(100%,#053F95));
	background:-webkit-linear-gradient(top,#2A568E 0%,#053F95 100%);
	background:-o-linear-gradient(top,#2A568E 0%,#053F95 100%);
	background:-ms-linear-gradient(top,#2A568E 0%,#053F95 100%);
	background:linear-gradient(top,#2A568E 0%,#053F95 100%);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#2A568E',endColorstr='#053F95',GradientType=0);
	padding:1px 13px;
	color:#fff;
	font-family:'Helvetica Neue',sans-serif;
	font-size:12px;
	border-radius:4px;
	-moz-border-radius:4px;
	-webkit-border-radius:4px;
	border:0px solid #CCCCCC
}

button:active, .buttonlnk:active {
	background:-moz-linear-gradient(top,#053F95 0%,#053F95 100%);
	background:-webkit-gradient(linear,left top,left bottom,color-stop(0%,#053F95),color-stop(100%,#053F95));
	background:-webkit-linear-gradient(top,#053F95 0%,#053F95 100%);
	background:-o-linear-gradient(top,#053F95 0%,#053F95 100%);
	background:-ms-linear-gradient(top,#053F95 0%,#053F95 100%);
	background:linear-gradient(top,#053F95 0%,#053F95 100%);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#053F95',endColorstr='#053F95',GradientType=0);
}


